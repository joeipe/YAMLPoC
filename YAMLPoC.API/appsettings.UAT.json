{
  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft.AspNetCore": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ApiSettings": "UAT",
  "ApiSuperSecret": "From-Vault",
  "KeyVault": {
    "VaultUri": "https://yamlpocvault-uat.vault.azure.net/"
  }
}
